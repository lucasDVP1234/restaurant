    <!-- Custom CSS for animations and styles -->
    <style>
    /* Keyframes for scrolling animations */
    @keyframes scroll {
        0% { transform: translateX(0%); }
        100% { transform: translateX(-50%); }
    }

    .animate-scroll {
        animation: scroll 30s linear infinite;
    }

    @keyframes scroll-creators {
        0% { transform: translateX(0%); }
        100% { transform: translateX(-50%); }
    }

    .animate-scroll-creators {
        animation: scroll-creators 40s linear infinite;
    }

    /* Gradient background */
    .bg-gradient {
        background: linear-gradient(135deg, #cbd7fa, #f9fafc);
    }

    /* Shadow effect */
    .shadow-card {
        box-shadow: 0 10px 20px rgba(0,0,0,0.1);
    }

    /* Rounded images */
    .rounded-img {
        border-radius: 20px;
    }
    @keyframes textAnimation {
    0%, 100% {
      background-position: 0% 50%;
    }
    50% {
      background-position: 100% 50%;
    }
  }

  .animate-text {
    background-size: 200% 200%;
    animation: textAnimation 5s ease-in-out infinite;
  }
    </style>
</head>
